package com.hniois.common.page;

import com.hniois.util.Const;

import java.util.ArrayList;
import java.util.List;

/**
 * 分页模型
 *
 * */
public class Page {
    private String id; // 主键id
    private String page; // 当前页数
    private String limit; // 分页大小
    private int offset; // 偏移量
    private int pagerSize;// 总页数
    private int pageSize = Const.PAGE;// 每页显示记录数

    //Layui分页参数
    private int code = 0; //
    private String msg = ""; //
    private int count; // 总数
    private List data = new ArrayList(); // 分页集合列表

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getPage() {
        return page;
    }

    public void setPage(String page) {
        this.page = page;
    }

    public String getLimit() {
        return limit;
    }

    public void setLimit(String limit) {
        this.limit = limit;
    }

    public int getOffset() {
        return offset;
    }

    public void setOffset(int offset) {
        this.offset = offset;
    }

    public int getPagerSize() {
        return pagerSize;
    }

    public void setPagerSize(int pagerSize) {
        this.pagerSize = pagerSize;
    }

    public int getPageSize() {
        return pageSize;
    }

    public void setPageSize(int pageSize) {
        this.pageSize = pageSize;
    }

    public int getCode() {
        return code;
    }

    public void setCode(int code) {
        this.code = code;
    }

    public String getMsg() {
        return msg;
    }

    public void setMsg(String msg) {
        this.msg = msg;
    }

    public int getCount() {
        return count;
    }

    public void setCount(int count) {
        this.count = count;
    }

    public List getData() {
        return data;
    }

    public void setData(List data) {
        this.data = data;
    }
}
