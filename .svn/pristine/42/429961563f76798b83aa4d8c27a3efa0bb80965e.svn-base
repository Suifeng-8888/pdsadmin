package com.hniois.entity.quartz;

import com.hniois.common.page.Page;

public class CheckQuartz extends Page {
    //id,username,msg,rdstate,cancel
    private String username;//接收消息的登录用户
    private String msg;//提示的消息信息
    private String rdstate;//是否已读提示信息 0已读,1未读,默认为1
    private String cancel;//判断客户点击取消提醒 0提醒,1取消 默认为0

    public String getUsername() {
        return username;
    }

    public void setUsername(String username) {
        this.username = username;
    }

    @Override
    public String getMsg() {
        return msg;
    }

    @Override
    public void setMsg(String msg) {
        this.msg = msg;
    }

    public String getRdstate() {
        return rdstate;
    }

    public void setRdstate(String rdstate) {
        this.rdstate = rdstate;
    }

    public String getCancel() {
        return cancel;
    }

    public void setCancel(String cancel) {
        this.cancel = cancel;
    }
}
