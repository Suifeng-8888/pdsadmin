package com.hniois.trace.entity;

import com.hniois.common.page.Page;

/**
 * 关键控制点表信息
 */
public class Key_Control extends Page {
    private Integer ctr_id;//关键控制点序号
    private String im_id;//进货id
    private String pro_name;//产品名称
    private String im_patch;//产品批次号
    private String ctr_name;//关键控制名称
    private String ctr_point;//关键控制点数及单位
    private String ctr_start;//控制开始时间
    private String ctr_end;//控制结束时间
    private String ctr_num;//监控次数
    private String person;//责任人
    private Integer result;//监控结论 0合格 1不合格

    //扩展字段
    private String pro_code;//产品编码

    public Integer getCtr_id() {
        return ctr_id;
    }

    public void setCtr_id(Integer ctr_id) {
        this.ctr_id = ctr_id;
    }

    public String getPro_name() {
        return pro_name;
    }

    public void setPro_name(String pro_name) {
        this.pro_name = pro_name;
    }

    public String getPro_code() {
        return pro_code;
    }

    public void setPro_code(String pro_code) {
        this.pro_code = pro_code;
    }

    public String getCtr_name() {
        return ctr_name;
    }

    public void setCtr_name(String ctr_name) {
        this.ctr_name = ctr_name;
    }

    public String getCtr_point() {
        return ctr_point;
    }

    public void setCtr_point(String ctr_point) {
        this.ctr_point = ctr_point;
    }

    public String getCtr_start() {
        return ctr_start;
    }

    public void setCtr_start(String ctr_start) {
        this.ctr_start = ctr_start;
    }

    public String getCtr_end() {
        return ctr_end;
    }

    public void setCtr_end(String ctr_end) {
        this.ctr_end = ctr_end;
    }

    public String getCtr_num() {
        return ctr_num;
    }

    public void setCtr_num(String ctr_num) {
        this.ctr_num = ctr_num;
    }

    public String getPerson() {
        return person;
    }

    public void setPerson(String person) {
        this.person = person;
    }

    public Integer getResult() {
        return result;
    }

    public void setResult(Integer result) {
        this.result = result;
    }

    public String getIm_id() {
        return im_id;
    }

    public void setIm_id(String im_id) {
        this.im_id = im_id;
    }

    public String getIm_patch() {
        return im_patch;
    }

    public void setIm_patch(String im_patch) {
        this.im_patch = im_patch;
    }
}
