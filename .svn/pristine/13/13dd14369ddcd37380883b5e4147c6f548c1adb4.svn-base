package com.hniois.trace.entity;

/**
 * 附件材料对象信息(临时中转的对象,无需操作对应的数据库)
 */
public class Attach_Material {
    private String attach_id;//附件材料序号
    private String mat_batch;//用料批次号
    private String mat_sup;//用料供应商
    private String sup_id;//供应商id
    private String use_num;//用料数量

    public String getAttach_id() {
        return attach_id;
    }

    public void setAttach_id(String attach_id) {
        this.attach_id = attach_id;
    }

    public String getMat_batch() {
        return mat_batch;
    }

    public void setMat_batch(String mat_batch) {
        this.mat_batch = mat_batch;
    }

    public String getMat_sup() {
        return mat_sup;
    }

    public void setMat_sup(String mat_sup) {
        this.mat_sup = mat_sup;
    }

    public String getSup_id() {
        return sup_id;
    }

    public void setSup_id(String sup_id) {
        this.sup_id = sup_id;
    }

    public String getUse_num() {
        return use_num;
    }

    public void setUse_num(String use_num) {
        this.use_num = use_num;
    }
}
