package com.hniois.entity.machine;

import com.hniois.common.page.Page;

/**
 * 仪器使用记录
 */
public class Record extends Page{
    /*基本属性
    ************************************** */
    private String mach_names;//仪器名称
    private String mach_ids;//仪器名称

    private String begin_time;//使用日期
    private String for_use;//用途
    private String people;//使用人
    private String status;//使用状态 0 正常,1 故障,
    private String  bz;//备注
    /*扩展属性
     ****************************************/

    public String getMach_names() {
        return mach_names;
    }

    public void setMach_names(String mach_names) {
        this.mach_names = mach_names;
    }

    public String getBegin_time() {
        return begin_time;
    }

    public void setBegin_time(String begin_time) {
        this.begin_time = begin_time;
    }

    public String getFor_use() {
        return for_use;
    }

    public void setFor_use(String for_use) {
        this.for_use = for_use;
    }


    public String getPeople() {
        return people;
    }

    public void setPeople(String people) {
        this.people = people;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getBz() {
        return bz;
    }

    public void setBz(String bz) {
        this.bz = bz;
    }

    public String getMach_ids() {
        return mach_ids;
    }

    public void setMach_ids(String mach_ids) {
        this.mach_ids = mach_ids;
    }
}
