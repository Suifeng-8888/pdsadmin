package com.hniois.entity.machine;

import com.hniois.common.page.Page;

/**
 * 仪器一览表
 */
public class Machine extends Page {
    private String mach_id;//仪器编号
    private String model;//仪器型号
    private String factory;//制造商
    private String out_num;//出厂编号
    private String attach;//附件
    private String mach_name;//仪器名称
    private String for_use;//用途
    private String arrive_time;//达到时间
    private String use_time;//启用时间
    private String address;//存放位置
    private String situation;//设备状况
    private String person;//责任人
    private String bz;//备注

    public String getMach_id() {
        return mach_id;
    }

    public void setMach_id(String mach_id) {
        this.mach_id = mach_id;
    }

    public String getModel() {
        return model;
    }

    public void setModel(String model) {
        this.model = model;
    }

    public String getFactory() {
        return factory;
    }

    public void setFactory(String factory) {
        this.factory = factory;
    }

    public String getOut_num() {
        return out_num;
    }

    public void setOut_num(String out_num) {
        this.out_num = out_num;
    }

    public String getAttach() {
        return attach;
    }

    public void setAttach(String attach) {
        this.attach = attach;
    }

    public String getMach_name() {
        return mach_name;
    }

    public void setMach_name(String mach_name) {
        this.mach_name = mach_name;
    }

    public String getFor_use() {
        return for_use;
    }

    public void setFor_use(String for_use) {
        this.for_use = for_use;
    }

    public String getArrive_time() {
        return arrive_time;
    }

    public void setArrive_time(String arrive_time) {
        this.arrive_time = arrive_time;
    }

    public String getUse_time() {
        return use_time;
    }

    public void setUse_time(String use_time) {
        this.use_time = use_time;
    }

    public String getAddress() {
        return address;
    }

    public void setAddress(String address) {
        this.address = address;
    }

    public String getSituation() {
        return situation;
    }

    public void setSituation(String situation) {
        this.situation = situation;
    }

    public String getPerson() {
        return person;
    }

    public void setPerson(String person) {
        this.person = person;
    }

    public String getBz() {
        return bz;
    }

    public void setBz(String bz) {
        this.bz = bz;
    }
}
